  "engines": {
    "node": "8.9.x",
    "npm": "5.5.x"
  },
  "scripts": {
    "postinstall": "./node_modules/@angular/cli/bin/ng build --prod ",
    "ng": "ng",
    "start": "node ./bin/www",
    "build": "ng build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "serve": "ng serve --proxy-config proxy.config.json",
    "e2e": "ng e2e",
    "dev": "echo 'run dev' && npm start",
    "prod": "ng build --prod && npm start",
    "nodemon": "ng build && ./node_modules/nodemon/bin/nodemon.js --config nodemon.json --exec npm run dev"
  },
  "private": true,
